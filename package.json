    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/trojs/validator/issues"
    },
    "homepage": "https://github.com/trojs/validator#readme",
    "devDependencies": {
        "@trojs/lint": "^0.2.9",
        "eslint": "^9.26.0",
        "globals": "^16.0.0",
        "jscpd": "^4.0.5"
    },
    "engines": {
        "node": ">= 20"
    },
    "keywords": [
        "objects",
        "validator"
    ],
    "funding": {
        "type": "github",
